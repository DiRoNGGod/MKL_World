<header class="header">
	<div class="header__content container">
		<a href="/" class="header__logo">
			<img src="/лого.png" alt="Логотип">
		</a>
		<nav class="header__nav">
			<a href="/">
				<li>Главная</li>
			</a>
			<a href="/gallery">
				<li>Галерея</li>
			</a>
			<a href="/wiki">
				<li>Вики</li>
			</a>
		</nav>
		<div class="dropdown">
			<input type="image" onclick="myFunction()" class="dropbtn" src="/user.png" alt="Аккаунт">
			<div id="myDropdown" class="dropdown-content">
				<% if (actions !== "Вход") { %>
					<a href="/profile">Профиль</a>
				<% } %>
				<a href="/reg"><%= actions %></a>
			</div>
		</div>
	</div>
</header>